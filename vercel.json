{
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "api/(.*)",
            "dest": "api/app.py"
        },
        {
            "src": "api/(.*)",
            "dest": "api/reviews.py"
        }
    ]
}
  